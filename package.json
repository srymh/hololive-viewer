{
  "name": "hololive-viewer",
  "version": "0.1.1",
  "private": true,
  "homepage": ".",
  "author": "srymh",
  "description": "",
  "main": "build/electron/main.js",
  "build": {
    "appId": "srymh.Hololive-Viewer",
    "productName": "Hololive-Viewer",
    "copyright": "Copyright Â© 2020 srymh",
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "axios": "^0.19.2",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "electron-store": "5.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-grid-layout": "^0.18.3",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"yarn proxy\" \"yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron --inspect=5858 .\"",
    "electron:build": "yarn build && tsc -p electron && electron-builder",
    "start": "cross-env BROWSER=NONE REACT_APP_DEVAPISERVER=http://localhost:8080 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "proxy": "node proxy.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-grid-layout": "^0.17.2",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "node-sass": "^4.14.1",
    "wait-on": "^5.1.0"
  }
}
